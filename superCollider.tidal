
let osc = OscShape {path = "/play",
                    params = [S "sound" Nothing,
                              F "mid1" (Just 0)
                             ],
                    timestamp = NoStamp,
                    latency = 0,
                    namedParams = False,
                    preamble = []
                   }

s <- stream "127.0.0.1" 57120 osc
let mid1 = makeF osc "mid1"


s $ sound "bd*12" |+| mid1 "0.1 0.4"

s $ silence

s $ sound "ho:1 bd:3*16 sn:5 sn bd*4"
